@import ('../../../../stylus/index.styl');

.banner {
    width: 100vw;
    height: $BannerHeight;
    background: $font-666;
    touch-action: pan-y !important;

    .slider {
        height: 100% !important;

        .slider-frame {
            height: 100% !important;

            .slider-list {
                height: 100% !important;

                .slider-slide {
                    height: 100% !important;

                    img {
                        height: 100% !important;
                        touch-action: pan-y !important;
                    }
                }
            }
        }
    }
}